{
    "_version": "1.32.0",
    "sap.app": {
        "id": "aapg.employeemgmt",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "ZEMPLOYEES_SRV": {
                "uri": "sap/opu/odata/sap/ZEMPLOYEES_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.97.2",
            "libs": {
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "aapg.employeemgmt.i18n.i18n"
                }
            },
            "createEmplConfig": {
				"type": "sap.ui.model.json.JSONModel",
                "uri": "model/json/CreateEmployeeConfig.json"
			},
            "employeesModel": {
                "dataSource": "ZEMPLOYEES_SRV",
                "preload": true,
                "settings": {
                    "useBatch": true,
                    "defaultBindingMode": "TwoWay"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "aapg.employeemgmt.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteLaunchpad",
                    "pattern": "",
                    "target": [
                        "TargetLaunchpad"
                    ]
                },
                {
                    "name": "RouteCreateEmployee",
                    "pattern": "createEmployee",
                    "target": [
                        "TargetCreateEmployee"
                    ]
                },
                {
                    "name": "RouteViewEmployee",
                    "pattern": "viewEmployee",
                    "target": ["TargetViewEmployee"]
                }
            ],
            "targets": {
                "TargetLaunchpad": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Launchpad",
                    "viewName": "Launchpad"
                },
                "TargetCreateEmployee": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "CreateEmployee",
                    "viewName": "CreateEmployee"
                },
                "TargetViewEmployee": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "ViewEmployee",
                    "viewName": "ViewEmployee"
                }
            }
        },
        "rootView": {
            "viewName": "aapg.employeemgmt.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        }
    }
}